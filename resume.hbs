<!doctype html>
<html>
  <head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

  <title>{{#resume.basics}}{{name}}{{/resume.basics}}</title>

  <link href="//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
  <link href="//cdn.bootcss.com/octicons/3.5.0/octicons.min.css" rel="stylesheet">

  <style type="text/css">
  {{{css}}}
  </style>

  <style media="print">
  {{{print}}}
  </style>

  </head>
  <body>

  <header>
    <div class="container">
      <div class="row">
        <div class="col-sm-9 col-sm-push-3">
          {{#resume.basics}}
            {{#name}}<h1>{{.}}</h1>{{/name}}
            {{#label}}<h2>{{.}}</h2>{{/label}}
          {{/resume.basics}}
        </div>
      </div>
    </div>
  </header>

  <main class="container">

  {{#resume.basics}}
    <section id="contact" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Contact"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#email}}
        <div class="col-sm-6">
          <strong>{{i18n "Email"}}</strong>
          <div class="email">
            <a href="mailto://{{.}}">{{.}}</a>
          </div>
        </div>
        {{/email}}
        {{#phone}}
        <div class="col-sm-6">
          <strong>{{i18n "Phone"}}</strong>
          <div class="phone">{{.}}</div>
        </div>
        {{/phone}}
        </div>
        <div class="row">
        {{#website}}
        <div class="col-sm-6">
          <strong>{{i18n "Website"}}</strong>
          <div class="website">
            <a href="{{.}}">{{.}}</a>
          </div>
        </div>
        {{/website}}
        </div>
      </div>
    </section>

    <section id="about" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "About"}}</h3>
      </aside>
      <div class="col-sm-9">
      {{#summary}}
        <p>{{.}}</p>
      {{/summary}}
      </div>
    </section>

    {{#if profiles.length}}
    <section id="profiles" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Profiles"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
          {{#profiles}}
          <div class="col-sm-6">
            {{#network}}
            <strong class="network">
              {{.}}
            </strong>
            {{/network}}
            {{#if username}}
            <div class="username">
              {{#if url}}
              <div class="url">
                <a href="{{url}}">{{username}}</a>
              </div>
              {{else}}
                {{username}}
              {{/if}}
            </div>
            {{else}}
              {{#if url}}
              <div class="url">
                <a href="{{url}}">{{url}}</a>
              </div>
              {{/if}}
            {{/if}}
          </div>
          {{/profiles}}
        </div>
      </div>
    </section>
    {{/if}}
  {{/resume.basics}}

    {{#if resume.work.length}}
    <section id="work" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Work"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#each resume.work}}
        <div class="col-sm-12 work">
          <h4 class="strike-through">
            <span>{{company}}</span>
            <span class="date">
              {{startDate}} — {{endDate}}
            </span>
          </h4>
          {{#website}}
          <div class="website pull-right">
            <a href="{{.}}">{{.}}</a>
          </div>
          {{/website}}
          {{#position}}
          <div class="position">
            {{.}}
          </div>
          {{/position}}
          {{#summary}}
          <div class="summary">
            <p>{{.}}</p>
          </div>
          {{/summary}}
          {{#if highlights.length}}
          <h4>{{i18n "Highlights"}}</h4>
          <ul class="highlights">
            {{#highlights}}
            <li class="bullet">{{.}}</li>
            {{/highlights}}
          </ul>
          {{/if}}
        </div>
        {{/each}}
        </div>
      </div>
    </section>
    {{/if}}


    {{#if resume.education.length}}
    <section id="education" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Education"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#each resume.education}}
        <div class="col-sm-12">
          <h4 class="strike-through">
            <span>{{institution}}</span>
            <span class="date">
              {{startDate}} — {{endDate}}
            </span>
          </h4>
          {{#area}}
          <div class="area">
            {{.}}
          </div>
          {{/area}}
          {{#studyType}}
          <div class="studyType">
            {{.}}
          </div>
          {{/studyType}}
          {{#if courses.length}}
          <h4>Courses</h4>
          <ul class="courses">
            {{#courses}}
            <li>{{.}}</li>
            {{/courses}}
          </ul>
          {{/if}}
        </div>
        {{/each}}
        </div>
      </div>
    </section>
    {{/if}}


    {{#if resume.publications.length}}
    <section id="publications" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Publications"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#each resume.publications}}
        <div class="col-sm-12">
          <h4 class="strike-through">
            <span>{{name}}</span>
            <span class="date">
              {{releaseDate}}
            </span>
          </h4>
          {{#website}}
          <div class="website pull-right">
            <a href="{{.}}"></a>
          </div>
          {{/website}}
          {{#publisher}}
          <div class="publisher">
            <em>{{i18n "Published by"}}</em>
            <strong>{{.}}</strong>
          </div>
          {{/publisher}}
          {{#summary}}
          <div class="summary">
            <p>{{.}}</p>
          </div>
          {{/summary}}
        </div>
        {{/each}}
        </div>
      </div>
    </section>
    {{/if}}

    {{#if resume.skills.length}}
    <section id="skills" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Skills"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#each resume.skills}}
        <div class="col-sm-6">
          {{#name}}
          <div class="name">
            <h4>{{.}}</h4>
          </div>
          {{/name}}
          {{#if keywords.length}}
          <ul class="keywords">
            {{#keywords}}
            <li>{{.}}</li>
            {{/keywords}}
          </ul>
          {{/if}}
        </div>
        {{/each}}
        </div>
      </div>
    </section>
    {{/if}}

    {{#if resume.languages.length}}
    <section id="languages" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Languages"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#each resume.languages}}
        <div class="col-sm-6">
          {{#language}}
          <div class="language">
            <strong>{{.}}</strong>
          </div>
          {{/language}}
          {{#fluency}}
          <div class="fluency">
            {{.}}
          </div>
          {{/fluency}}
        </div>
        {{/each}}
        </div>
      </div>
    </section>
    {{/if}}

    {{#if resume.interests.length}}
    <section id="interests" class="row">
      <aside class="col-sm-3">
        <h3>{{i18n "Interests"}}</h3>
      </aside>
      <div class="col-sm-9">
        <div class="row">
        {{#each resume.interests}}
        <div class="col-sm-6">
          {{#name}}
          <div class="name">
            <h4>{{.}}</h4>
          </div>
          {{/name}}
          {{#if keywords.length}}
          <ul class="keywords">
            {{#keywords}}
            <li>{{.}}</li>
            {{/keywords}}
          </ul>
          {{/if}}
        </div>
        {{/each}}
        </div>
      </div>
    </section>
    {{/if}}

  </main>

  </body>
</html>
